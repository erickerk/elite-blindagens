{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "name": "elite-blindagens",
  "buildCommand": "",
  "framework": null,
  "cleanUrls": true,
  "trailingSlash": false,
  "functions": {
    "app/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "VERCEL_TOOLBAR": "0"
  },
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/favicon.svg",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*).svg",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).png",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).jpg",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/veiculos-venda",
      "destination": "/veiculos-venda.html"
    },
    {
      "source": "/admin",
      "destination": "/admin.html"
    }
  ]
}
